<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

<% articles.forEach ( function ( article ) { %>

    <div class="familyname" postId="<%= article._id %>"><a href="post/<%= article._id.toHexString() %>"><%= article.familyname %></a></div>
    <div class="story"><%= article.story %></div>

<% } ) %>

  </body>
</html>